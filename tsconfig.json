{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@scripts/*": [
        "src/scripts/*"
      ],
      "@layout/*": [
        "src/layout/*"
      ],
      "@public/*": [
        "public/*"
      ],
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}